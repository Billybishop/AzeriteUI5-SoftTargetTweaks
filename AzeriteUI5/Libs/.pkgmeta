package-as: AzeriteUI5
enable-nolib-creation: no

externals:
  Libs/LibStub: https://repos.wowace.com/wow/libstub/tags/1.0
  Libs/CallbackHandler-1.0: https://repos.wowace.com/wow/callbackhandler/trunk/CallbackHandler-1.0
  Libs/Ace3: https://repos.wowace.com/wow/ace3/trunk
  Libs/LibEditMode:
    url: https://github.com/p3lim-wow/LibEditMode
    tag: latest
  Libs/oUF:
    url: https://github.com/oUF-wow/oUF
    tag: latest
  Libs/LibSharedMedia-3.0: https://repos.wowace.com/wow/libsharedmedia-3-0/trunk/LibSharedMedia-3.0

embedded-libraries:
    - ace3

optional-dependencies:
    - ace3
    - clique

tools-used:
    - ouf

move-folders:
    AzeriteUI5/AzeriteUI5: AzeriteUI5

manual-changelog:
    filename: CHANGELOG.md
    markup-type: markdown

license-output: LICENSE.txt

ignore:
    - CHANGELOG.md
    - CODE_OF_CONDUCT.md
    - LICENSE.md
    - README.md
